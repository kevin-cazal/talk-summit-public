{
  "name": "pokered-wasmboy-demo",
  "version": "1.0.0",
  "description": "Pokemon Red emulator using WasmBoy",
  "scripts": {
    "serve": "npx http-server -p 8080 -c-1",
    "serve:demo1": "npx http-server -p 8080 -c-1",
    "serve:demo2": "npx http-server -p 8081 -c-1",
    "serve:demo3": "npx http-server -p 8082 -c-1",
    "serve:demo4": "npx http-server -p 8083 -c-1",
    "serve:all": "npx concurrently -n demo1,demo2,demo3,demo4 -c blue,green,yellow,magenta \"npm run serve:demo1\" \"npm run serve:demo2\" \"npm run serve:demo3\" \"npm run serve:demo4\""
  },
  "dependencies": {
    "wasmboy": "^0.7.1",
    "idb": "^2.1.3"
  },
  "devDependencies": {
    "http-server": "^14.1.1",
    "concurrently": "^8.2.2"
  }
}
